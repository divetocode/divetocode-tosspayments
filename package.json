{
  "name": "@divetocode/tosspayments",
  "version": "0.9.1",
  "description": "A TypeScript wrapper around @tosspayments/tosspayments-sdk for React, Next.js, and Node.js server integrations.",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./client": {
      "types": "./dist/client.d.ts",
      "import": "./dist/client.mjs",
      "require": "./dist/client.cjs"
    },
    "./server": {
      "types": "./dist/server.d.ts",
      "import": "./dist/server.mjs",
      "require": "./dist/server.cjs"
    },
    "./types": {
      "types": "./dist/types.d.ts",
      "default": "./dist/types.d.ts"
    }
  },
  "type": "module",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "tosspayments",
    "toss",
    "payments",
    "payment-gateway",
    "react",
    "nextjs",
    "nodejs",
    "wrapper",
    "sdk",
    "divetocode"
  ],
  "author": {
    "name": "divetocode",
    "email": "divetocode.official@gmail.com",
    "url": "https://divetocode.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/divetocode/divetocode-tosspayments"
  },
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "devDependencies": {
    "@types/node": "^24.3.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "tsup": "^8.5.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@tosspayments/tosspayments-sdk": "^2.3.7"
  }
}